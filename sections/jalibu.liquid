<div class="page-width">
    <h1 class="alert alert-primary" role="alert"> {{- section.settings.jalibuId -}} </h1>
    <h2> {{ 'blog.article.written_by' | t: author: article.author }} </h2>

    <p> {{- section.settings.jalibuParagraph -}} </p>
    {% comment %} <h3 class="testing-menu-handles"> {{ pages.title }} </h3> {% endcomment %}
    
    <ul class="menu-tests"> 
        {% for link in linklists.secondary-menu.links %}
              <li>{{- link.title -}}</li>
        {% endfor %}
  </ul>
</div>


{%- style -%}
    .AirBnB{
        background-color: darkturquoise;
    }

    .menu-tests{
      display: flex;
    }

{%- endstyle -%}




{% schema %}
{
  "name": "Jalibu Sectioni",
  "tag": "section",
  "class": "AirBnB",
  "settings": [
    {
      "type": "text",
      "id": "jalibuId",
      "label": "JalibuHeader"
    },
    {
        "type": "text",
        "id": "jalibuParagraph",
        "label": "Jalibu Paragraph"
      }
  ]
}
{% endschema %}
