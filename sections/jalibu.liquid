<div class="page-width">
    <h1 class="alert alert-primary" role="alert"> {{- section.settings.jalibuId -}} </h1>
    {% assign productTests = Product.handle %}
    <h3> {{ product.title | handle }} </h3>
    

    <p> {{- section.settings.jalibuParagraph -}} </p>
    {% comment %} <h3 class="testing-menu-handles"> {{ pages.title }} </h3> {% endcomment %}
    

    {% comment %} Creeated this menu just for tests... Now sending it to maain menu in header section {% endcomment %}
    {% comment %} <ul class="menu-tests"> 
        {% for link in linklists.secondary-menu.links %}
              <li>{{- link.title -}}</li>
        {% endfor %}
    </ul> {% endcomment %}
</div>



{%- style -%}
    .AirBnB{
        background-color: darkturquoise;
    }

    .menu-tests {
    display: flex;
    justify-content: space-around;
}

{%- endstyle -%}




{% schema %}
{
  "name": "Jalibu Sectioni",
  "tag": "section",
  "class": "AirBnB",
  "settings": [
    {
      "type": "text",
      "id": "jalibuId",
      "label": "JalibuHeader"
    },
    {
        "type": "text",
        "id": "jalibuParagraph",
        "label": "Jalibu Paragraph"
      }
  ]
}
{% endschema %}
